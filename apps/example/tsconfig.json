{
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "types": ["jest", "node"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "**/*.jsx",
    ".next/types/**/*.ts",
    "dist/fid/.next/types/**/*.ts",
    "next-env.d.ts",
    "src/env.mjs",
    "next.config.mjs",
    "../../.next/types/**/*.ts",
    "../../dist/fid/.next/types/**/*.ts"
  ],
  "extends": "../../tsconfig.base.json"
}
